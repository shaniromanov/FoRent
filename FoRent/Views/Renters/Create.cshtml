@model FoRent.Models.Renter

@{
    ViewData["Title"] = "Create";
}
<br />
<br />
<br />
<h1><b>יצירת דירה</b></h1>
<br />
<h4>פרטים אישיים</h4>
<br />
<hr />
<br />
<br />
<div class="row">
   
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group float-label-control">
                <label asp-for="password" class="control-label"></label><br />
                <input asp-for="password" class="form-control" /><br />
                <span asp-validation-for="password" class="text-danger"></span><br />
            </div>
            <div class="form-group float-label-control">
                <label asp-for="Username" class="control-label"></label><br />
                <input asp-for="Username" class="form-control" /><br />
                <span asp-validation-for="Username" class="text-danger"></span><br />
            </div>
            <div class="form-group float-label-control">
                <label asp-for="FirstName" class="control-label"></label><br />
                <input asp-for="FirstName" class="form-control" /><br />
                <span asp-validation-for="FirstName" class="text-danger"></span><br />
            </div>
            <div class="form-group float-label-control">
                <label asp-for="LastName" class="control-label"></label><br />
                <input asp-for="LastName" class="form-control" /><br />
                <span asp-validation-for="LastName" class="text-danger"></span><br />
            </div>
            <div class="form-group float-label-control">
                <label asp-for="Phone" class="control-label"></label><br />
                <input asp-for="Phone" class="form-control" /><br />
                <span asp-validation-for="Phone" class="text-danger"></span><br />
            </div>
            <div class="form-group float-label-control">
                <label asp-for="Mail" class="control-label"></label><br />
                <input asp-for="Mail" class="form-control" /><br />
                <span asp-validation-for="Mail" class="text-danger"></span><br />
            </div>
            <br />

            <div class="form-group float-label-control label-bottom">
                <input type="submit" value="המשך" class="btn btn-default" />
            </div>
        </form>
    
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
